{
  "functions": {
    "source": ".output/server",
    "runtime": "nodejs16"
  },
  "hosting": {
    "predeploy": [
      "pnpm run build && cd .output/server && pnpm i"
    ],
    "public": ".output/public",
    "cleanUrls": true,
    "rewrites": [
      {
        "source": "**",
        "function": "server"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}